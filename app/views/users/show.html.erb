
<% if logged_in? %>
  <h2> <%= @user.name %> </h2>

  <div class="row">
    <div class="col-md-4 ">
      <%= render 'events/event_form' %>
  </div>

  <div class="col-md-8">
      <%= render @events %>
  </div>
</div>

<% else %>
  <h2> Please sign in. <h2>
  <%= link_to "Sign in", login_path %>
<% end %>





